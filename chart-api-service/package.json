{
  "name": "econ-graph-chart-api",
  "version": "1.0.0",
  "description": "Private Chart API Service for EconGraph MCP Server Integration",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .js",
    "lint:fix": "eslint src tests --ext .js --fix",
    "build": "echo 'No build step required for Node.js service'",
    "docker:build": "docker build -t econ-graph-chart-api .",
    "docker:run": "docker run -p 3001:3001 econ-graph-chart-api"
  },
  "keywords": [
    "chart",
    "api",
    "mcp",
    "econ-graph",
    "chart.js",
    "visualization"
  ],
  "author": "EconGraph Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/econ-graph2.git",
    "directory": "chart-api-service"
  }
}
