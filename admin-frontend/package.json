{
  "name": "econ-graph-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "/admin",
  "description": "Administrative interface for EconGraph platform - INTERNAL USE ONLY",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "5.18.0",
    "@mui/material": "5.18.0",
    "@mui/x-data-grid": "6.20.4",
    "@mui/x-date-pickers": "6.20.2",
    "@types/lodash": "4.17.20",
    "@types/node": "20.19.13",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "ace-builds": "1.43.3",
    "ajv": "8.17.1",
    "axios": "^1.12.2",
    "chart.js": "4.5.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "date-fns": "3.6.0",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-ace": "10.1.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.3.1",
    "react-json-view": "1.21.3",
    "react-query": "3.39.3",
    "react-router-dom": "6.30.1",
    "react-scripts": "5.0.1",
    "typescript": "5.9.2",
    "web-vitals": "3.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "jest": "27.5.1"
  },
  "scripts": {
    "start": "REACT_APP_ENV=admin react-scripts start",
    "build": "REACT_APP_ENV=admin react-scripts build",
    "test": "NODE_OPTIONS='--max-old-space-size=2048' react-scripts test",
    "test:serial": "NODE_OPTIONS='--max-old-space-size=2048' react-scripts test --runInBand --watchAll=false",
    "test:memory-optimized": "NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=64' jest --maxWorkers=1 --runInBand",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=2048' jest --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "NODE_OPTIONS='--max-old-space-size=1024' eslint src --ext .ts,.tsx --max-warnings 20",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=1024' eslint src --ext .ts,.tsx --fix",
    "lint:memory-optimized": "NODE_OPTIONS='--max-old-space-size=512' eslint src --ext .ts,.tsx --max-warnings 20 --cache --cache-location .eslintcache",
    "prettier-check": "NODE_OPTIONS='--max-old-space-size=512' prettier --check 'src/**/*.{js,ts,tsx,css}'",
    "prettier-fix": "NODE_OPTIONS='--max-old-space-size=512' prettier --write 'src/**/*.{js,ts,tsx,css}'",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=1024' tsc --noEmit"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/__tests__/**",
      "!src/**/__mocks__/**",
      "!src/setupTests.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.test.ts",
          "**/*.test.tsx",
          "**/__tests__/**/*"
        ],
        "rules": {
          "testing-library/no-wait-for-multiple-assertions": "off",
          "testing-library/no-wait-for-side-effects": "off",
          "testing-library/no-node-access": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/d3-dispatch": "3.0.4"
  },
  "resolutions": {
    "@types/d3-dispatch": "3.0.4"
  },
  "proxy": "http://localhost:8081"
}
